Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintfull

*loginShell: true
*scrollBar: false
*secondaryScroll: true
*saveLines: 65535
*cursorBlink: false
*urgentOnBell: true
*highlightSelection: true

URxvt*termName: rxvt-256color
URxvt*depth: 32
URxvt*font: xft:DejaVu Sans Mono:pixelsize=14
URxvt*boldFont: xft:DejaVu Sans Mono:pixelsize=14:style=Bold

! special
*.foreground:   #d0d0d0
*.background:   #151515
*.cursorColor:  #d0d0d0

! black
*.color0:       #151515
*.color8:       #505050

! red
*.color1:       #ac4142
*.color9:       #ac4142

! green
*.color2:       #90a959
*.color10:      #90a959

! yellow
*.color3:       #f4bf75
*.color11:      #f4bf75

! blue
*.color4:       #6a9fb5
*.color12:      #6a9fb5

! magenta
*.color5:       #aa759f
*.color13:      #aa759f

! cyan
*.color6:       #75b5aa
*.color14:      #75b5aa

! white
*.color7:       #d0d0d0
*.color15:      #f5f5f5
